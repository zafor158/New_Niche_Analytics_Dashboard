# Production Environment Variables
NODE_ENV=production
PORT=5000

# Database - You'll need to replace this with your production database URL
DATABASE_URL="postgresql://username:password@your-production-db-host:5432/niche_analytics"

# JWT - Use a strong secret in production
JWT_SECRET="your-super-secure-production-jwt-secret-key-here"
JWT_EXPIRES_IN="7d"

# CORS - Replace with your production frontend URL
FRONTEND_URL="https://your-frontend-app.vercel.app"
